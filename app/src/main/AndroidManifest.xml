<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jacksonhuang.backgroundsample">

    <application
        android:name="com.example.jacksonhuang.backgroundsample.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.jacksonhuang.backgroundsample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.jacksonhuang.backgroundsample.ThreadActivity"/>
        <activity android:name="com.example.jacksonhuang.backgroundsample.AsyncTaskActivity"/>
        <activity android:name="com.example.jacksonhuang.backgroundsample.IntentServiceActivity"/>
        <activity android:name=".RxJavaActivity"/>
        <service android:name=".MyIntentService"/>

    </application>

</manifest>